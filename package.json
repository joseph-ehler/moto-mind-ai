    "test:watch": "jest --watch",
    "test:isolation": "jest tests/isolation",
    "test:integration": "jest tests/integration",
    "db:migrate": "node -r ts-node/register scripts/run-migrations.ts",
    "db:seed": "node -r ts-node/register scripts/seed.ts",
    "db:seed:smartphone": "node -r ts-node/register scripts/seed-smartphone.ts",
    "demo:reset": "npm run db:seed:smartphone",
    "demo:full": "npm run db:migrate && npm run db:seed:smartphone && npm run dev"
